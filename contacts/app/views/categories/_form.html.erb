<%= form_with(model: @category) do |form| %>
  <%= render 'shared/errors', obj: @category %>

  <div class="field">
    <strong>
      <%= form.label :name, 'category name' %>
    </strong>
    <%= form.text_field :name %>
  </div>

  <div class="actions">
    <%= form.submit class:"btn btn-success" %>
  </div>
  
<% end %>